{
  "extends": "athom",
  "rules": {
    "brace-style": "off",
    "camelcase": "off",
    "consistent-return": "off",
    "eqeqeq": "off",
    "func-names": "off",
    "global-require": "off",
    "guard-for-in": "off",
    "import/extensions": "off",
    "no-async-promise-executor": "off",
    "no-cond-assign": "off",
    "no-console": "off",
    "no-const-assign": "off",
    "no-const-assignt": "off",
    "no-constant-condition": "off",
    "no-new": "off",
    "no-restricted-syntax": "off",
    "no-tabs": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "node/no-unsupported-features/node-builtins": "off",
    "radix": "off",
    "valid-typeof": "off"
  }
}
